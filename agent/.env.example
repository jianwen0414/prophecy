# Prophecy Agent Environment Configuration
# Copy this file to .env and fill in the values

# Required: Gemini API Key for AI Council
# Get from: https://aistudio.google.com/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Required: NFT.storage API Key for IPFS pinning
# Get from: https://nft.storage/manage/
NFT_STORAGE_KEY=your_nft_storage_key_here

# Solana RPC URL (defaults to devnet)
RPC_URL=https://api.devnet.solana.com

# Agent's Solana keypair (one of these methods)
# Option 1: Path to keypair file
AGENT_KEYPAIR_PATH=~/.config/solana/id.json

# Option 2: Base58 encoded private key (alternative to path)
# AGENT_PRIVATE_KEY=your_base58_private_key_here

# Server port for agent API
PORT=3001
